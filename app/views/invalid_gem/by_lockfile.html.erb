<h2>Upload lockfile</h2>
<%= link_to "by_list", invalid_gem_by_list_path %>

<%= form_tag(invalid_gem_by_lockfile_path, multipart: true, method: :post) do %>
  <div>
    <%= file_field_tag "lockfile" %>
  </div>
  <%= submit_tag "send" %>
<% end %>

<%= render "dangerous_gems", gems: @dangerous_gems %>
